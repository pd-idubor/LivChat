<%- include("partials/header"); -%>

<%- include("partials/head"); -%>

<div class="container">
	<div class="p-5 my-4 bg-light rounded-3">
		<form id="createPost" class="py-3">
			<div class="mb-5">
				<label class="form-label" for="inputUsername">Title</label>
					<input type="title" class="form-control" id="inputTitle" placeholder="Subject">
			</div>
			<div class="mb-3">
				<label class="form-label" for="content">Content</label>
				<textarea type="textarea" class="form-control" rows="5" id="inputContent" placeholder="What are your thoughts?"></textarea>
			</div>
			<button type="submit" class="btn btn-success">Save post</button>
		</form>
	</div>
</div>


<%- include("partials/footer"); -%>
